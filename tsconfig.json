{
	"extends": "./.svelte-kit/tsconfig.json",
	"exclude": [
		"node_modules",
		".svelte-kit",
		"build",
		"dist",
		".vercel",
		".netlify"
	  ],
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"module": "esnext",
        "moduleResolution": "node"
	}
}
